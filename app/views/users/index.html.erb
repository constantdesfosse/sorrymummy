<h1>All Tattooists</h1>

<% @users.each do |user| %>
    <li>
      <%=image_tag user.picture%>
      <%=user.first_name.capitalize + " " + user.last_name.upcase %>
      <button>
      <%= link_to user_path(user) do %>
        <i class="fa fa-user"></i> <%= t ".profile", default: "Profile" %>
      <% end %>
      </button>
    </li>
<% end %>
