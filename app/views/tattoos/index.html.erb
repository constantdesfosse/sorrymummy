<div class="container">
  <h1>Tattoos(<%= @tattoos.count %>)</h1>
  <div id="tag_cloud">
    <% tag_cloud Tattoo.tag_counts, %w[s m l] do |tag, css_class| %>
    <%= link_to "#" + tag.name, tag_path(tag.name), class: css_class %>
    <% end %>
  </div>
      <%= render 'tattoos/tattoos_cards_all' %>
</div>
